{
  "channels": [
    {
      "name": "/devbox/aos",
      "type": "aos.logging.LogMessageFbs",
      "source_node": "devbox",
      "frequency": 200,
      "num_senders": 20,
      "max_size": 2048
    },
    {
      "name": "/remote/aos",
      "type": "aos.logging.LogMessageFbs",
      "source_node": "remote",
      "frequency": 200,
      "num_senders": 20,
      "max_size": 2048
    },
    {
      "name": "/devbox/aos",
      "type": "aos.message_bridge.Timestamp",
      "source_node": "devbox",
      "frequency": 100,
      "num_senders": 2,
      "max_size": 200,
      "destination_nodes": [
        {
          "name": "remote",
          "priority": 1,
          "time_to_live": 5000000,
          "timestamp_logger": "LOCAL_AND_REMOTE_LOGGER",
          "timestamp_logger_nodes": ["devbox"]
        }
      ]
    },
    {
      "name": "/remote/aos",
      "type": "aos.message_bridge.Timestamp",
      "source_node": "remote",
      "frequency": 100,
      "num_senders": 2,
      "max_size": 200,
      "destination_nodes": [
        {
          "name": "devbox",
          "priority": 1,
          "time_to_live": 5000000,
          "timestamp_logger": "LOCAL_AND_REMOTE_LOGGER",
          "timestamp_logger_nodes": ["remote"]
        }
      ]
    },
    {
      "name": "/devbox/aos",
      "type": "aos.message_bridge.ServerStatistics",
      "source_node": "devbox",
      "frequency": 4
    },
    {
      "name": "/remote/aos",
      "type": "aos.message_bridge.ServerStatistics",
      "source_node": "remote",
      "frequency": 4
    },
    {
      "name": "/devbox/aos",
      "type": "aos.message_bridge.ClientStatistics",
      "source_node": "devbox",
      "frequency": 100
    },
    {
      "name": "/remote/aos",
      "type": "aos.message_bridge.ClientStatistics",
      "source_node": "remote",
      "frequency": 100
    },
    {
      "name": "/devbox/aos/remote_timestamps/remote",
      "type": "aos.message_bridge.RemoteMessage",
      "frequency": 200,
      "logger": "NOT_LOGGED",
      "source_node": "devbox"
    },
    {
      "name": "/remote/aos/remote_timestamps/devbox",
      "type": "aos.message_bridge.RemoteMessage",
      "frequency": 200,
      "logger": "NOT_LOGGED",
      "source_node": "remote"
    },
    {
      "name": "/devbox/aos",
      "type": "aos.timing.Report",
      "source_node": "devbox",
      "frequency": 50,
      "num_senders": 20,
      "max_size": 2048
    },
    {
      "name": "/devbox/aos",
      "type": "aos.logging.DynamicLogCommand",
      "source_node": "devbox",
      "frequency": 10,
      "num_senders": 1
    },
    {
      "name": "/remote/aos",
      "type": "aos.timing.Report",
      "source_node": "remote",
      "frequency": 50,
      "num_senders": 20,
      "max_size": 2048
    },
    {
      "name": "/remote/aos",
      "type": "aos.logging.DynamicLogCommand",
      "source_node": "remote",
      "frequency": 10,
      "num_senders": 1
    },
    {
      "name": "/test",
      "type": "aos.examples.Ping",
      "source_node": "devbox",
      "frequency": 2000,
      "destination_nodes": [
        {
          "name": "remote",
          "priority": 1,
          "time_to_live": 5000000,
          "timestamp_logger": "LOCAL_AND_REMOTE_LOGGER",
          "timestamp_logger_nodes": ["devbox"]
        }
      ]
    },
    {
      "name": "/test",
      "type": "aos.examples.Pong",
      "source_node": "remote",
      "frequency": 2000,
      "destination_nodes": [
        {
          "name": "devbox",
          "priority": 1,
          "time_to_live": 5000000,
          "timestamp_logger": "LOCAL_AND_REMOTE_LOGGER",
          "timestamp_logger_nodes": ["remote"]
        }
      ]
    },
    {
      "name": "/not_used",
      "type": "aos.examples.Pong",
      "source_node": "remote",
      "frequency": 2000,
      "destination_nodes": [
        {
          "name": "devbox",
          "priority": 1,
          "time_to_live": 5000000,
          "timestamp_logger": "LOCAL_AND_REMOTE_LOGGER",
          "timestamp_logger_nodes": ["remote"]
        }
      ]
    },
    {
      "name": "/reliable",
      "type": "aos.examples.Ping",
      "source_node": "devbox",
      "destination_nodes": [
        {
          "name": "remote",
          "priority": 1,
          "timestamp_logger": "LOCAL_AND_REMOTE_LOGGER",
          "timestamp_logger_nodes": ["devbox"],
          "time_to_live": 0
        }
      ]
    },
    {
      "name": "/unreliable",
      "type": "aos.examples.Ping",
      "source_node": "devbox",
      "destination_nodes": [
        {
          "name": "remote",
          "priority": 1,
          "timestamp_logger": "LOCAL_LOGGER",
          "time_to_live": 5000000
        }
      ]
    }
  ],
  "maps": [
    {
      "match": {
        "name": "/aos*",
        "source_node": "devbox"
      },
      "rename": {
        "name": "/devbox/aos"
      }
    },
    {
      "match": {
        "name": "/aos*",
        "source_node": "remote"
      },
      "rename": {
        "name": "/remote/aos"
      }
    }
  ],
  "nodes": [
    {
      "name": "devbox",
      "hostname": "devbox",
      "port": 4646
    },
    {
      "name": "remote",
      "hostname": "vpu",
      "port": 4646
    }
  ],
  "applications": [
    {
      "name": "ping2",
      "maps": [
        {
          "match": {
            "name": "/test"
          },
          "rename": {
            "name": "/test2"
          }
        }
      ]
    },
    {
      "name": "pong2",
      "maps": [
        {
          "match": {
            "name": "/test"
          },
          "rename": {
            "name": "/test2"
          }
        }
      ]
    }
  ]
}
